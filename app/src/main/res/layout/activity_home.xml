<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.HomeActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fl_bottommenunav"
        android:layout_above="@+id/bottom_menu_navigation"/>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bottom_menu_navigation"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        app:itemIconTint="@color/primary_upper"
        app:itemTextColor="@color/primary_upper"
        app:menu="@menu/bottom_menu_navigation"
        />

</RelativeLayout>